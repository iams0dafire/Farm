<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/icbackg"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="rmuti.farm.MainActivity">

    <RelativeLayout
        android:id="@+id/layout1"
        android:layout_width="match_parent"
        android:layout_height="140dp">


        <ImageView
            android:id="@+id/headpic1"
            android:layout_width="270dp"
            android:layout_height="120dp"
            android:layout_alignParentLeft="true"
            android:src="@drawable/txarduino" />

        <ImageView
            android:id="@+id/headpic2"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:onClick="onClickedtoinputdata"
            android:layout_alignParentRight="true"
            android:src="@drawable/butplus" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout2"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_below="@+id/layout1">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout3"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_below="@+id/layout2">

        <LinearLayout
            android:id="@+id/layout31"
            android:layout_width="175dp"
            android:layout_height="100dp">

            <RelativeLayout
                android:id="@+id/layout311"
                android:layout_width="175dp"
                android:layout_height="100dp">

                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/icfertilizer" />

            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout32"
            android:layout_width="175dp"
            android:layout_height="100dp"
            android:layout_toRightOf="@+id/layout31">

            <RelativeLayout
                android:id="@+id/layout312"
                android:layout_width="175dp"
                android:layout_height="100dp">

                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/iccare" />

            </RelativeLayout>

        </LinearLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout4"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_below="@+id/layout3">

        <LinearLayout
            android:id="@+id/linear41"
            android:layout_width="175dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/linear1">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:src="@drawable/txfertilizer" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear42"
            android:layout_width="175dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/linear2"
            android:layout_toRightOf="@+id/linear41">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:src="@drawable/txcare" />

        </LinearLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout5"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_below="@+id/layout4">

        <LinearLayout
            android:id="@+id/linear4"
            android:layout_width="175dp"
            android:layout_height="100dp">

            <RelativeLayout
                android:id="@+id/linear411"
                android:layout_width="175dp"
                android:layout_height="100dp">

                <ImageView
                    android:id="@+id/pic4"
                    android:layout_width="100dp"
                    android:layout_height="100dp"

                    android:layout_centerHorizontal="true"
                    android:src="@drawable/icweather" />

            </RelativeLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear5"
            android:layout_width="175dp"
            android:layout_height="100dp"
            android:layout_toRightOf="@+id/linear4">

            <RelativeLayout
                android:id="@+id/linear412"
                android:layout_width="175dp"
                android:layout_height="100dp">

                <ImageView
                    android:id="@+id/pic5"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_centerInParent="true"
                    android:src="@drawable/icharvest" />
            </RelativeLayout>
        </LinearLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout6"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/layout5">

        <LinearLayout
            android:id="@+id/linear64"
            android:layout_width="175dp"
            android:layout_height="55dp"
            android:layout_alignBottom="@+id/linear4">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:src="@drawable/txweather" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear65"
            android:layout_width="175dp"
            android:layout_height="55dp"
            android:layout_alignBottom="@+id/linear5"
            android:layout_toRightOf="@+id/linear64">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:src="@drawable/txharvest" />

        </LinearLayout>


    </RelativeLayout>


</RelativeLayout>
